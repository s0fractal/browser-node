{
  "watch": [
    "main.js",
    "preload.js",
    "index.html",
    "agents/**/*",
    "intents/**/*",
    "scripts/**/*"
  ],
  "ext": "js,html,css,json",
  "ignore": [
    "node_modules/**/*",
    "dist/**/*",
    ".git/**/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "js": "electron"
  }
}